<h1>SubjectData#show</h1>
<p>Find me in app/views/subject_data/show.html.erb</p>

<% @json = JSON.parse(@subject_datum.payload) %>
<% if @json %>
  <% @json["data"].map do |key, data| %>
    <%= render partial: "subject_data_tile", locals: {key: key, data: data} %>
  <% end %> 
<% end %>

<br>
<%= link_to "Back", subject_path(@subject_datum.subject) %>
